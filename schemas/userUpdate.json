{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "email"
    },
    "interests": {
      "type": "string",
      "maxLength": 250

    },
    "hobbies": {
      "type": "string",
      "maxLength": 250
    },
    "zip": {
      "type": "string",
      "minLength": 5,
      "maxLength": 15
    }, 
    "radius": {
      "type": "string",
      "minLength": 1, 
      "maxLength": 100
    }
  },
  "additionalProperties": false,
  "required": []
}
